<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.xinyo.subtitle.mapper.SearchHistoryMapper">
    <update id="timesIncr">
        update douban_search_history set search_times = search_times + 1
        where keyword = #{keyword}
    </update>
</mapper>
